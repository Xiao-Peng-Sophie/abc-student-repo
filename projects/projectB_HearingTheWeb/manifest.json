{
  "name": "Hearing the Web",
  "description": "Create a 'blind' browsing experience.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tts"
  ],
  "content_scripts":[
    {
       "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon":
    "icon128.png",

    "default_popup":"popup/popup.html"

  }
}
